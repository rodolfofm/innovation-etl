  <step>
    <name>Ler pedido (PEDID)</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>INNOVATION-FB</connection>
    <sql>
	SELECT 
	p.ID_PEDIDO 
	,p.PEDCODIGO 
	,p.PEDDTEMIS
	,p.PEDPZETGSIS
	,p.PEDDTSAIDA
	,p.CLICODIGO
	,p.ENDCODIGO 
	,p.FUNCODIGO2 AS FUN_VENDEDOR
	,p.FUNCODIGO  AS FUN_ATENDENTE
	,sum(i.PDPUNITLIQUIDO * i.PDPQTDADE) AS PEDVRTOTAL --, mas o correto é multiplicar a quantidade vezes o valor unitário = PDPRD.PDPUNITLIQUIDO * PDPRD.PDPQTDADE
	,p.PEDDTBAIXA
	,p.PEDSITPED  --(A = ABERTO, B = BAIXADO, F = FATURADO, C = CANCELADO)
	,p.TPCODIGO  --(Cadastro de tipos de pedidos na tabela TPPEDID)
	,p.PEDORIGEM 
	,p.TRACODIGO1 -- (FK da tabela TRANS (Transportadora) )
	,p.PEDTPFRETE --: 0, EMITENTE, 1 , DEST/REMETENTE, 2, TERCEIROS, 9, SEM FRETE
	,n.NFCODIGO -- nr nota
	FROM PEDID p 
		LEFT JOIN PDNF n ON p.EMPCODIGO = n.EMPCODIGO AND p.ID_PEDIDO = n.ID_PEDIDO  and n.PDNSITUACAO='A'
        INNER JOIN PDPRD i on p.ID_PEDIDO = i.ID_PEDIDO   
	WHERE p.EMPCODIGO = 2 
		AND p.PEDSITPED='F' 
		AND p.PEDDTBAIXA > '2019-01-01'
	GROUP BY 
	p.ID_PEDIDO 
	,p.PEDCODIGO 
	,p.PEDDTEMIS
	,p.PEDPZETGSIS
	,p.PEDDTSAIDA
	,p.CLICODIGO
	,p.ENDCODIGO 
	,p.FUNCODIGO2 
	,p.FUNCODIGO  
	,p.PEDDTBAIXA
	,p.PEDSITPED  --(A = ABERTO, B = BAIXADO, F = FATURADO, C = CANCELADO)
	,p.TPCODIGO  --(Cadastro de tipos de pedidos na tabela TPPEDID)
	,p.PEDORIGEM 
	,p.TRACODIGO1 -- (FK da tabela TRANS (Transportadora) )
	,p.PEDTPFRETE --: 0, EMITENTE, 1 , DEST/REMETENTE, 2, TERCEIROS, 9, SEM FRETE
	,n.NFCODIGO -- nr nota
ORDER BY p.ID_PEDIDO </sql>
