    <name>Ler pedido (PDPRD)</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>INNOVATION-FB</connection>
    <sql>SELECT 
r.RECCODIGO 
,r.RECNRDOC
,r.RECPARCELA
,r.RECDTEMISSAO
,r.RECDTVENCTO
,r.RECDTPREVIS
,r.RECDTDOC
,r.CLICODIGO
,r.COBCODIGO
,r.RECTIPODOCTO
,r.STCODIGO --(Tabela STATUSTIT contem os dados de status) 
,r.RECVALOR
,r.RECVRDESCONTO
,r.RECVRJUROS
,r.RECVALORABERTO
,max(rx.REBDTLIQ) REBDTLIQ 
,sum(rx.REBVALOR) AS REBVALOR
,sum(rx.REBVRDESC) AS REBVRDESC
,sum(rx.REBVRJUROS) AS REBVRJUROS
FROM RECEB r 
	LEFT JOIN RECBX rx ON r.EMPCODIGO = rx.EMPCODIGO AND r.RECCODIGO = rx.RECCODIGO 
WHERE r.EMPCODIGO = 2
AND r.RECSITUACAO ='N'
--AND r.CLICODIGO = 15
GROUP BY 
r.RECCODIGO 
,r.RECNRDOC
,r.RECPARCELA
,r.RECDTEMISSAO
,r.RECDTVENCTO
,r.RECDTPREVIS
,r.RECDTDOC
,r.CLICODIGO
,r.COBCODIGO
,r.RECTIPODOCTO
,r.STCODIGO --(Tabela STATUSTIT contem os dados de status) 
,r.RECVALOR
,r.RECVRDESCONTO
,r.RECVRJUROS
,r.RECVALORABERTO</sql>
